FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      sample;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type sets;
libs            ("libsampling.so");

interpolationScheme cell;

setFormat       raw;

sets
(
    top_wall 
    {
        type    lineCell;
        axis    xyz;
        start   (0 0.99 -.025);
        end     (3 0.99 -.025);
    }
);

fields          ( p );


